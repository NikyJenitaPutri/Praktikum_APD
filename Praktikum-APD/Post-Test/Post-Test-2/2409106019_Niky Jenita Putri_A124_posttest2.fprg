    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="namalengkap, namapanggilan, prodi, universitas, tempatlahir, tanggallahir" type="String" array="False" size=""/>
            <declare name="umur, TigaAngkaTerakhirNim, hasilmodulus" type="Integer" array="False" size=""/>
            <declare name="nim, tinggibadan" type="Real" array="False" size=""/>
            <output expression="&quot;masukan nama lengkap&quot;"/>
            <input variable="namalengkap"/>
            <output expression="&quot;masukan nama panggilan&quot;"/>
            <input variable="namapanggilan"/>
            <output expression="&quot;masukan nama prodi&quot;"/>
            <input variable="prodi"/>
            <output expression="&quot;masukan umur&quot;"/>
            <input variable="umur"/>
            <output expression="&quot;masukan NIM&quot;"/>
            <input variable="NIM"/>
            <output expression="&quot;masukan universitas&quot;"/>
